<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <p>こんにちは、<span><%= link_to "#{current_user.name}さん", "/users/#{current_user.id}", class: :greeting__link%></span></p>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes  %>
    </div>
  </div>
</main>
